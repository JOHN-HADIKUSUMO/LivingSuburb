@model LivingSuburb.Models.JobDetail
@{
    ViewData["Title"] = "Jobs ";
}
<!--=== Breadcrumbs ===-->
<div class="breadcrumbs">
    <div class="container">
        <h1 class="pull-left">Job Detail</h1>
        <ul class="pull-right breadcrumb">
            <li><a href="/Jobs">Jobs</a></li>
            <li class="active">Job Detail</li>
        </ul>
    </div>
</div><!--/breadcrumbs-->
<div class="container content">
    <div class="row  ht450">
        <div class="col-md-12 text-left">
            <h1>@Html.Raw(Model.Title)</h1>
            <h3>@Html.Raw(Model.Company)</h3>
            <h3>@Html.Raw((string.IsNullOrEmpty(Model.Suburb) ? "" : (Model.Suburb + ", ")) + Model.State)</h3>
        </div>
        <div class="col-md-12 text-justify">
            @Html.Raw(Model.FullDescription)
        </div>
    </div>
</div>
@section Scripts{
    <script type="text/javascript" src="/assets/js/tag.js"></script>
    <script type="text/javascript" src="/assets/js/common.js"></script>
    <script type="text/javascript" src="/assets/js/state.js"></script>
    <script type="text/javascript" src="/assets/js/suburb.js"></script>
    <script type="text/javascript" src="/assets/js/jobCategory.js"></script>
    <script type="text/javascript" src="/assets/js/jobSubCategory.js"></script>
    <script type="text/javascript" src="/assets/js/job.js"></script>
    <script type="text/javascript">
        var appModule = angular.module('appModule', ['jobModule']);
    </script>
}