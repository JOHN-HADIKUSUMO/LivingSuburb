@{
    ViewData["Title"] = "Not Allowed";
}
<!--=== Breadcrumbs ===-->
<div class="breadcrumbs">
    <div class="container">
        <h1 class="pull-left">@Html.Raw(TempData["code"].ToString())</h1>
        <ul class="pull-right breadcrumb">
            <li><a href="/">ERROR</a></li>
            <li class="active">@Html.Raw(TempData["code"].ToString())</li>
        </ul>
    </div>
</div><!--/breadcrumbs-->
<div class="container content">
    <div class="col-md-12 text-center ht450">
        <section>
            <h2>@Html.Raw(TempData["Label"].ToString())</h2>
        </section>
    </div>
</div>
@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
    <script type="text/javascript">
        var appModule = angular.module('appModule', []);
    </script>
}
